int led = 13;
int pushButton1 = 3;
int pushButton2 = 2;
void setup() {
  pinMode(led,OUTPUT);
  pinMode(pushButton1,INPUT);
  pinMode(pushButton2,INPUT);
}

void loop() {
if(digitalRead(pushButton1)==HIGH)
{digitalWrite(led,HIGH);
  delay(2000);}
  else if(digitalRead(pushButton2)==HIGH)
  {digitalWrite(led,LOW);
  delay(2000);
  }
}
