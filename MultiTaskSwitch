int sw;
void setup() {
  // put your setup code here, to run once:
pinMode(11,OUTPUT);
pinMode(12,OUTPUT);
pinMode(13,OUTPUT);
pinMode(10,INPUT);
}

void loop() {
  // put your main code here, to run repeatedly:
sw=digitalRead(10);
if(sw==1)
{
  digitalWrite(11,1);
  digitalWrite(12,1);
  digitalWrite(13,1);
  }
  else
  {
   digitalWrite(11,0);
  digitalWrite(12,0);
  digitalWrite(13,0); 
    }
}
